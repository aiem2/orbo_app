<div id="<%= dom_id content %>">
  <p>
    <strong>Title:</strong>
    <%= content.title %>
  </p>

  <p>
    <strong>Date:</strong>
    <%= content.date %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= content.description %>
  </p>

  <p>
    <strong>Download:</strong>
    <%= content.download %>
  </p>

  <p>
    <strong>City:</strong>
    <%= content.city %>
  </p>

  <p>
    <strong>State:</strong>
    <%= content.state %>
  </p>

</div>
